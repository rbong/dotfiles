colorscheme solarized_dark
nno K gt
nno J gT
nno j 3j
nno k 3k
nno <C-j> j
nno <C-k> k
nno < :tabmove! -1<CR>
nno > :tabmove! +1<CR>
" wip
" set editor="osascript -e 'tell application \"Terminal\" to do script \"nvim\"'"
set focuscontent
set hintmatching="fuzzy"
nno <ESC> :set nohlsearch<CR><ESC>
nno / :set hlsearch<CR>/
nno ? :set hlsearch<CR>?
nno n :set hlsearch<CR>n
nno N :set hlsearch<CR>N
set gui=nonavigation
nno <leader>u :set um!<CR>
set urlseparator=","
nno $ :tablast<CR>
nno 0 :tabfirst<CR>
" goes to the first non-app tab - trying to find alternative
nno ^ 4gt
nno <C-f> /
nno <M-f> /
