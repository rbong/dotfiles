conky.config = {
    out_to_console = true,
    out_to_x = false,
    background = false,
    update_interval = 2,
    total_run_times = 0,
    use_spacer = 'none'
}

conky.text = [[ ${cpu cpu1}% ${cpu cpu2}% $memperc%  ${fs_free /}  ${acpitemp}c  ${wireless_essid wlan0} ${wireless_link_qual_perc wlan0}%  ${exec amixer get Master | grep Mono: | cut -d " " -f6,8}  ${battery_short}  ${time %a %b %d %I:%M%P}]]
