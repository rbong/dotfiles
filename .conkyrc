conky.config = {
    out_to_console = true,
    out_to_x = false,
    background = false,
    update_interval = 2,
    total_run_times = 0,
    use_spacer = 'none'
}

conky.text = [[c ${cpu cpu1}% / ${cpu cpu2}% m $memperc% :: ${fs_used /} / ${fs_size /} :: t ${acpitemp}c :: w ${wireless_essid wlp2s0} ${wireless_link_qual_perc wlp2s0}% :: v ${exec amixer -c 0 get Master | grep Mono: | cut -d " " -f6,8} :: b ${battery} :: t ${time %a %b %d %I:%M%P}]]
